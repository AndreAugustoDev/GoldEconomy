bank {
    balance {
        help show your balance
        user player
        hook balance
    }

    deposit string:arg? {
        help deposits money
        hook deposit
        user player

    }

    withdraw string:arg? {
        help withdraws money
        hook withdraw
        user player
    }

    pay string:arg int:arg {
        help pay a player money
        hook pay
        user player
    }
}